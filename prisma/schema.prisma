generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Query {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  data      String
  isUser    Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
